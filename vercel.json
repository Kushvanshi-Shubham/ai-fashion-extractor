{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "env": {
    "VITE_APP_NAME": "AI Fashion Attribute Extractor",
    "VITE_APP_VERSION": "2.0.0-vlm",
    "VITE_MAX_FILE_SIZE": "15728640",
    "VITE_MAX_CONCURRENT_EXTRACTIONS": "5",
    "VITE_ENABLE_LOGGING": "false",
    "VITE_DEV_MODE": "false",
    "VITE_ENABLE_PERFORMANCE_METRICS": "true",
    "VITE_ENABLE_VLM_STATUS": "true",
    "VITE_DEFAULT_ENHANCED_MODE": "true",
    "VITE_SHOW_MODEL_INFO": "true",
    "VITE_SHOW_CONFIDENCE_SCORES": "true",
    "VITE_ENABLE_DISCOVERY_MODE": "true",
    "VITE_DEFAULT_DISCOVERY_MODE": "false",
    "VITE_SHOW_DISCOVERIES_IN_TABLE": "true",
    "VITE_ENABLE_ANALYTICS": "true",
    "VITE_TRACK_EXTRACTION_TIME": "true",
    "VITE_TRACK_TOKEN_USAGE": "true",
    "VITE_ENABLE_IMAGE_CACHE": "true",
    "VITE_CACHE_EXPIRATION_MINUTES": "60",
    "VITE_ENABLE_HMR": "false",
    "VITE_ENABLE_SOURCE_MAPS": "false",
    "VITE_DETAILED_ERRORS": "false",
    "VITE_MOCK_API": "false",
    "VITE_SENTRY_ENABLED": "true",
    "VITE_ENVIRONMENT": "production",
    "VITE_SENTRY_DEBUG": "false"
  },
  "build": {
    "env": {
      "NODE_OPTIONS": "--max_old_space_size=4096"
    }
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
