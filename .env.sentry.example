# Sentry Configuration
# Get your DSN from https://sentry.io/settings/projects/

# Enable Sentry monitoring (set to 'true' to enable)
VITE_SENTRY_ENABLED=false

# Your Sentry DSN (Data Source Name)
# Example: https://examplePublicKey@o0.ingest.sentry.io/0
VITE_SENTRY_DSN=

# Environment name (development, staging, production)
VITE_ENVIRONMENT=development

# Application version (for release tracking)
VITE_APP_VERSION=1.0.0

# Enable Sentry debug mode (logs to console in development)
VITE_SENTRY_DEBUG=false

# API URL for tracing
VITE_API_URL=http://localhost:5000

# Source map upload configuration (for production builds)
VITE_SENTRY_ORG=your-sentry-org
VITE_SENTRY_PROJECT=your-frontend-project
SENTRY_AUTH_TOKEN=your-auth-token-here
